{
  "type": "sqlite",
  "database": "data.sqlite",
  "synchronize": false,
  "logging": false,
  "entities": [
    "src/entity/**/*.ts"
  ],
  "migrations": [
    "src/migrations/sqlite/**/*.ts"
  ],
  "migrationsTransactionMode": "each",
  "cli": {
    "migrationsDir": "src/migrations/sqlite"
  }
}
